javascript: (function() {
    function toArray(c) {
        let k, a = new Array;
        for (k = 0; k < c.length; ++k) a[k] = c[k];
        return a;
    }
    let img, altText, images = toArray(document.images);
    for (let i = 0; i < images.length; ++i) {
        img = images[i];
        altText = document.createTextNode(img.alt);
        img.parentNode.replaceChild(altText, img)
    }
})();
