!function(){const t=document.images;let e,i;for(let i=0;i<t.length;++i)w=h=t.naturalHeight,t[i].getAttribute("width")||t[i].getAttribute("height")||(t[i].setAttribute("width",t[i].naturalWidth),t[i].setAttribute("height",t[i].naturalWidth),e=new URL(t[i].src),t[i].src=e.pathname);i=document.querySelector(".post-content").innerHTML.replaceAll("http://localhost:4000","{{site.url}}"),navigator.clipboard.writeText(i).then((()=>{alert("Copied")}))}();
